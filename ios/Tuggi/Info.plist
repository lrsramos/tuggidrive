<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Tuggi</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>$(PRODUCT_NAME)</string>
	<key>CFBundlePackageType</key>
	<string>$(PRODUCT_BUNDLE_PACKAGE_TYPE)</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0.0</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>tuggi</string>
				<string>com.tuggi.app</string>
			</array>
		</dict>
	</array>
	<key>CFBundleVersion</key>
	<string>1</string>
	<key>GMSApiKey</key>
	<string>YOUR_IOS_GOOGLE_MAPS_API_KEY</string>
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>
	<key>LSMinimumSystemVersion</key>
	<string>12.0</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<false/>
		<key>NSAllowsLocalNetworking</key>
		<true/>
	</dict>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>Tuggi needs your location to provide navigation assistance and find nearby points of interest, even when the app is in the background.</string>
	<key>NSLocationAlwaysUsageDescription</key>
	<string>Tuggi needs continuous access to your location to provide real-time navigation and proximity alerts, even when the app is not active.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Tuggi needs your location to help you navigate and discover nearby points of interest while using the app.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Tuggi needs microphone access to enable voice commands and audio descriptions for points of interest.</string>
	<key>NSMotionUsageDescription</key>
	<string>Tuggi needs motion data to improve navigation accuracy and detect your movement patterns.</string>
	<key>NSUserActivityTypes</key>
	<array>
		<string>$(PRODUCT_BUNDLE_IDENTIFIER).expo.index_route</string>
	</array>
	<key>UIBackgroundModes</key>
	<array>
		<string>location</string>
		<string>fetch</string>
		<string>processing</string>
	</array>
	<key>BGTaskSchedulerPermittedIdentifiers</key>
	<array>
		<string>com.tuggi.app.refresh</string>
		<string>com.tuggi.app.processing</string>
		<string>com.tuggi.app.background.processing</string>
		<string>com.tuggi.app.background.refresh</string>
	</array>
	<key>UILaunchStoryboardName</key>
	<string>SplashScreen</string>
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>arm64</string>
	</array>
	<key>UIRequiresFullScreen</key>
	<false/>
	<key>UIStatusBarStyle</key>
	<string>UIStatusBarStyleDefault</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UIUserInterfaceStyle</key>
	<string>Automatic</string>
	<key>UIViewControllerBasedStatusBarAppearance</key>
	<false/>
	<key>NSCameraUsageDescription</key>
	<string>Tuggi needs camera access to let you take photos for your profile and add images to points of interest.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Tuggi needs access to your photo library to let you choose profile pictures and add images to points of interest.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>Tuggi needs permission to save photos to your library when you download POI images.</string>
	<key>NSRemoteNotificationUsageDescription</key>
	<string>Tuggi sends notifications about nearby points of interest and important updates to enhance your experience.</string>
</dict>
</plist>
